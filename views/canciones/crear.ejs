<%- include ../layouts/cabecera.ejs %> 
<br>
<div class="card">
    <div class="card-header">
        Formulario de Captura
    </div>
    <div class="card-body">
        <h4 class="card-title"> Datos de musica </h4>
        <p class="card-text"> 
            <form method="post" action="/canciones">

                <div class="form-group">
                    <br>
                  <label for=""> Nombre: </label>
                  <input type="text" name="nombre" id="my-input" class="form-control">
                  </div>
                <div class="form-group">
                    <label for=""> Grupo: </label>
                    <input type="text" name="grupo" id="my-input" class="form-control">
                </div>
            
                <br>
                <button type="submit" class="btn btn-success"> Guardar </button>
                <a class="btn btn-danger" href="/canciones" role="button"> Cancelar </a>
            </form>
            
        </p>
    </div>
</div>


